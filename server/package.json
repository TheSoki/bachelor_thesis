{
    "name": "server",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --max-warnings=0",
        "migrate:format": "prisma format",
        "migrate": "prisma migrate dev",
        "migrate:up": "prisma migrate deploy",
        "migrate:seed": "tsx src/scripts/db-seed.ts",
        "migrate:seed:test": "tsx src/scripts/db-seed-test.ts",
        "migrate:reset": "prisma migrate reset",
        "generate": "prisma generate --no-hints",
        "postinstall": "prisma generate --no-hints",
        "local": "docker-compose -f docker-compose.local.yaml up -d",
        "local:remove": "docker-compose -f docker-compose.local.yaml down --remove-orphans",
        "format": "prettier --write .",
        "typecheck": "tsc --noEmit",
        "validateDependencyTree": "tsx src/scripts/validateDependencyTree.ts",
        "integrate": "pnpm run lint && pnpm run typecheck && pnpm run validateDependencyTree",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.0",
        "@prisma/client": "5.20.0",
        "@radix-ui/react-dialog": "^1.1.2",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-select": "^2.1.2",
        "@radix-ui/react-slot": "^1.1.0",
        "@tanstack/react-query": "^5.59.13",
        "@tanstack/react-query-devtools": "^5.59.13",
        "@trpc/client": "11.0.0-next-beta.318",
        "@trpc/next": "11.0.0-next-beta.318",
        "@trpc/react-query": "11.0.0-next-beta.318",
        "@trpc/server": "11.0.0-next-beta.318",
        "bcrypt": "^5.1.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.5",
        "lucide-react": "^0.452.0",
        "next": "14.2.15",
        "next-auth": "^4.24.8",
        "prisma": "^5.20.0",
        "puppeteer": "^23.5.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "reflect-metadata": "^0.2.2",
        "sonner": "^1.5.0",
        "superjson": "^2.2.1",
        "tailwind-merge": "^2.5.3",
        "tailwindcss-animate": "^1.0.7",
        "ts-pattern": "^5.4.0",
        "tsx": "^4.19.1",
        "typedi": "^0.10.0",
        "winston": "^3.15.0",
        "zod": "^3.23.8",
        "zustand": "5.0.0-rc.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.48.1",
        "@svgr/webpack": "^8.1.0",
        "@types/bcrypt": "^5.0.2",
        "@types/node": "^22.7.5",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "@typescript-eslint/eslint-plugin": "^8.8.1",
        "autoprefixer": "^10.4.20",
        "dpdm": "^3.14.0",
        "eslint": "^8.0.0",
        "eslint-config-next": "14.2.15",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "tailwindcss": "^3.4.13",
        "typescript": "^5.6.3"
    },
    "packageManager": "pnpm@9.1.0",
    "engines": {
        "node": ">=20.10.0",
        "pnpm": ">=9.1.0",
        "npm": "use-pnpm",
        "yarn": "use-pnpm"
    }
}
