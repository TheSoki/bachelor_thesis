{
    "name": "server",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --max-warnings=0",
        "migrate:format": "prisma format",
        "migrate": "prisma migrate dev",
        "migrate:up": "prisma migrate deploy",
        "migrate:seed": "tsx src/scripts/db-seed.ts",
        "migrate:seed:test": "tsx src/scripts/db-seed-test.ts",
        "migrate:reset": "prisma migrate reset",
        "generate": "prisma generate --no-hints",
        "postinstall": "prisma generate --no-hints",
        "local": "docker-compose -f docker-compose.local.yaml up -d",
        "local:remove": "docker-compose -f docker-compose.local.yaml down --remove-orphans",
        "format": "prettier --write .",
        "typecheck": "tsc --noEmit",
        "validateDependencyTree": "tsx src/scripts/validateDependencyTree.ts",
        "integrate": "pnpm run lint && pnpm run typecheck && pnpm run validateDependencyTree",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.0.1",
        "@prisma/client": "6.7.0",
        "@radix-ui/react-dialog": "^1.1.13",
        "@radix-ui/react-label": "^2.1.6",
        "@radix-ui/react-select": "^2.2.4",
        "@radix-ui/react-slot": "^1.2.2",
        "@tanstack/react-query": "^5.75.6",
        "@tanstack/react-query-devtools": "^5.75.6",
        "@trpc/client": "11.1.2",
        "@trpc/next": "11.1.2",
        "@trpc/react-query": "11.1.2",
        "@trpc/server": "11.1.2",
        "bcrypt": "^5.1.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^16.5.0",
        "lucide-react": "^0.509.0",
        "next": "15.3.2",
        "next-auth": "^4.24.11",
        "prisma": "^6.7.0",
        "puppeteer": "^24.8.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.56.3",
        "reflect-metadata": "^0.2.2",
        "sonner": "^2.0.3",
        "superjson": "^2.2.2",
        "tailwind-merge": "^3.2.0",
        "tailwindcss-animate": "^1.0.7",
        "ts-pattern": "^5.7.0",
        "tsx": "^4.19.4",
        "typedi": "^0.10.0",
        "winston": "^3.17.0",
        "zod": "^3.24.4",
        "zustand": "5.0.4"
    },
    "devDependencies": {
        "@playwright/test": "^1.52.0",
        "@svgr/webpack": "^8.1.0",
        "@types/bcrypt": "^5.0.2",
        "@types/node": "^22.15.17",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.3",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "autoprefixer": "^10.4.21",
        "dpdm": "^3.14.0",
        "eslint": "^9.26.0",
        "eslint-config-next": "15.3.2",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^v3.4.17",
        "typescript": "^5.8.3"
    },
    "packageManager": "pnpm@9.1.0",
    "engines": {
        "node": ">=20.10.0",
        "pnpm": ">=9.1.0",
        "npm": "use-pnpm",
        "yarn": "use-pnpm"
    }
}
